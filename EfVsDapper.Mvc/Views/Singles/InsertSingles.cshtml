@using EntityFrameworkVsCoreDapper.Results
@model IEnumerable<ResultView>

<div style="display:flex">
    <div class="mr-lg-5">Input Single Objects</div>

    <div class="mr-lg-5">Count Products: @ViewBag.CountProducts</div>
    <div>Count Customers: @ViewBag.CountCustomers</div>
</div>
<last-result last-result="@ViewBag.LastResult"></last-result>

<table class="table table-striped">
    <thead>
        <tr>
            <th style="width: 10%" scope="col">#</th>
            <th style="width: 30%" scope="col">Dapper</th>
            <th style="width: 30%" scope="col">Ef 6</th>
            <th style="width: 30%" scope="col">Ef Core</th>
            @*<th style="width: 23.75%" scope="col">Ef Core AsNoTracking Sql Command</th>*@
        </tr>
    </thead>
    <tbody>
        @{
            var interac = 0;
            foreach (var item in Model)
            {
                <tr>
                    <td>@item.EFCore.Interactions</td>

                    <cell-result controller-name="Singles" interaction-element="@interac" action-retour="InsertSingles" action-name="InsertProductDapper" item-result-view="@item.Dapper"></cell-result>
                    <cell-result controller-name="Singles" interaction-element="@interac" action-retour="InsertSingles" action-name="InsertProductEf6" item-result-view="@item.Ef6"></cell-result>
                    <cell-result controller-name="Singles" interaction-element="@interac" action-retour="InsertSingles" action-name="InsertProductEfCore" item-result-view="@item.EFCore"></cell-result>
                    @*<cell-result controller-name="Singles" interaction-element="@interac" action-retour="InsertSingles" action-name="InsertProductEfCoreAsNoTrackingHardSql" item-result-view="@item.EfCoreAsNoTrackingHardSql"></cell-result>*@
                </tr>
                interac++;
            }
        }


    </tbody>
</table>



