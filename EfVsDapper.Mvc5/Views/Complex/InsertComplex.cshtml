@using EntityFrameworkVsCoreDapperNetFramework.Results
@using EfVsDapper.Mvc5.TagHelpers
@model IEnumerable<ResultView>

<div style="display:flex">
    <div class="mr-lg-5">Inserts Single Objects</div>

    <div class="mr-lg-5">Count Products: @ViewBag.CountProducts</div>
    <div>Count Customers: @ViewBag.CountCustomers</div>
</div>

<last-result last-result="@ViewBag.LastResult"></last-result>

<table class="table table-striped">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">Dapper</th>
            <th scope="col">Ef 6</th>
            <th scope="col">Ef Core</th>
            <th scope="col">Ef Core AsNoTracking Sql Command</th>
        </tr>
    </thead>
    <tbody>

        @foreach (var product in Model)
        {
            <tr>
                <td>@product.EFCore.Interactions</td>

                @Html.GetCellResultTag(product.Dapper, "Complex", "InsertCustomerDapper")
                @Html.GetCellResultTag(product.Ef6, "Complex", "InsertCustomerEf6")
                @Html.GetCellResultTag(product.EFCore, "Complex", "InsertCustomerEfCore")
                @Html.GetCellResultTag(product.EfCoreAsNoTracking, "Complex", "InsertCustomerEfCoreAsNoTrackingHardSql")
            </tr>
        }

    </tbody>
</table>



